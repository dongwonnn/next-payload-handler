{
  "name": "next-payload-handler",
  "version": "0.3.5",
  "description": "Custom handling of Next.js Data Cache.",
  "keywords": [
    "next cacheHandler",
    "data cache",
    "cache synchronization",
    "multi Instance",
    "custom cache key"
  ],
  "homepage": "https://github.com/dongwonnn/next-payload-handler#readme",
  "bugs": {
    "url": "https://github.com/dongwonnn/next-payload-handler/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dongwonnn/next-payload-handler.git"
  },
  "license": "MIT",
  "author": {
    "name": "Dongwon Kim",
    "email": "kkum9408@gmail.com"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest -w"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@google-cloud/common": "^5.0.2",
    "@google-cloud/storage": "^7.15.0",
    "@types/node": "^22.7.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.3.3",
    "redis": "^4.7.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vitest": "^2.1.2"
  },
  "peerDependencies": {
    "next": "^13.4.0 || ^14.0.0 || ^15.0.0"
  }
}
